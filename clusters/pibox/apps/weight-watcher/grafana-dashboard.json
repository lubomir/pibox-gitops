{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Overview",
      "type": "row"
    },
    {
      "title": "Up",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "options": { "0": { "text": "DOWN", "color": "red" }, "1": { "text": "UP", "color": "green" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      },
      "options": { "colorMode": "background", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "expr": "up{job=\"weight-watcher-service\"}", "legendFormat": "{{pod}}", "refId": "A" }
      ]
    },
    {
      "title": "Total Requests (last 24h)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 5, "x": 4, "y": 1 },
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] } }, "overrides": [] },
      "options": { "colorMode": "background", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "expr": "sum(increase(api_http_requests_total{job=\"weight-watcher-service\"}[24h]))", "legendFormat": "", "refId": "A" }
      ]
    },
    {
      "title": "Error Rate (5xx)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 5, "x": 9, "y": 1 },
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.01 }, { "color": "red", "value": 0.05 }] }
        },
        "overrides": []
      },
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "expr": "sum(rate(api_http_requests_total{job=\"weight-watcher-service\", status=~\"5..\"}[$__rate_interval])) / sum(rate(api_http_requests_total{job=\"weight-watcher-service\"}[$__rate_interval]))", "legendFormat": "", "refId": "A" }
      ]
    },
    {
      "title": "Avg Response Time",
      "type": "stat",
      "gridPos": { "h": 4, "w": 5, "x": 14, "y": 1 },
      "id": 4,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 1 }] }
        },
        "overrides": []
      },
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "expr": "sum(rate(api_http_requests_duration_seconds_sum{job=\"weight-watcher-service\"}[$__rate_interval])) / sum(rate(api_http_requests_duration_seconds_count{job=\"weight-watcher-service\"}[$__rate_interval]))", "legendFormat": "", "refId": "A" }
      ]
    },
    {
      "title": "Request Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 5, "x": 19, "y": 1 },
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] }
        },
        "overrides": []
      },
      "options": { "colorMode": "background", "graphMode": "area", "reduceOptions": { "calcs": ["lastNotNull"] } },
      "targets": [
        { "expr": "sum(rate(api_http_requests_total{job=\"weight-watcher-service\"}[$__rate_interval]))", "legendFormat": "", "refId": "A" }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "id": 101,
      "title": "Request Rate",
      "type": "row"
    },
    {
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "id": 10,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "fillOpacity": 20, "stacking": { "mode": "normal" } } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "calcs": ["mean", "max"] } },
      "targets": [
        { "expr": "sum by (exported_endpoint) (rate(api_http_requests_total{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval]))", "legendFormat": "{{exported_endpoint}}", "refId": "A" }
      ]
    },
    {
      "title": "Request Rate by Status Code",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "id": 11,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "drawStyle": "line", "fillOpacity": 20, "stacking": { "mode": "normal" } } },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": "2.." }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": "3.." }, "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": "4.." }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byRegexp", "options": "5.." }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "options": { "legend": { "displayMode": "table", "calcs": ["mean", "max"] } },
      "targets": [
        { "expr": "sum by (status) (rate(api_http_requests_total{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval]))", "legendFormat": "{{status}}", "refId": "A" }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 102,
      "title": "Latency",
      "type": "row"
    },
    {
      "title": "Average Response Time by Endpoint",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 15 },
      "id": 20,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "fillOpacity": 10 } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "calcs": ["mean", "max"] } },
      "targets": [
        { "expr": "sum by (exported_endpoint) (rate(api_http_requests_duration_seconds_sum{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval])) / sum by (exported_endpoint) (rate(api_http_requests_duration_seconds_count{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval]))", "legendFormat": "{{exported_endpoint}}", "refId": "A" }
      ]
    },
    {
      "title": "Request Duration Heatmap",
      "type": "heatmap",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 15 },
      "id": 21,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": {} },
      "options": {
        "calculate": false,
        "yAxis": { "unit": "s" },
        "color": { "scheme": "Oranges" },
        "cellGap": 1
      },
      "targets": [
        { "expr": "sum(increase(api_http_requests_duration_seconds_bucket{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval])) by (le)", "legendFormat": "{{le}}", "refId": "A", "format": "heatmap" }
      ]
    },
    {
      "title": "P50 / P90 / P99 Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 23 },
      "id": 22,
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "unit": "s", "custom": { "drawStyle": "line", "fillOpacity": 10 } }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "calcs": ["mean", "max"] } },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum(rate(api_http_requests_duration_seconds_bucket{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval])) by (le))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.90, sum(rate(api_http_requests_duration_seconds_bucket{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval])) by (le))", "legendFormat": "p90", "refId": "B" },
        { "expr": "histogram_quantile(0.99, sum(rate(api_http_requests_duration_seconds_bucket{job=\"weight-watcher-service\", exported_endpoint!=\"/metrics\"}[$__rate_interval])) by (le))", "legendFormat": "p99", "refId": "C" }
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 39,
  "tags": ["weight-watcher", "prometheus"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "title": "Weight Watcher",
  "uid": "weight-watcher-dashboard"
}
